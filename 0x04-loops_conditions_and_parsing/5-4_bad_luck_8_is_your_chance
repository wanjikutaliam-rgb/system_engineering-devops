#!/usr/bin/env bash
# This script prints messages based on the loop iteration: 4="bad luck", 8="good luck", others="Best School"
count=1
while [ $count -le 10 ]; do
    if [ $count -eq 4 ]; then
        echo "bad luck"
    elif [ $count -eq 8 ]; then
        echo "good luck"
    else
        echo "Best School"
    fi
    count=$((count + 1))
done
